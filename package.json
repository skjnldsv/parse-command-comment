		"@types/node": "^12.20.18",
		"@typescript-eslint/eslint-plugin": "^2.7.0",
		"@typescript-eslint/parser": "^2.7.0",
		"@vercel/ncc": "^0.34.0",
		"eslint": "^6.6.0",
		"eslint-config-prettier": "^6.6.0",
		"eslint-plugin-prettier": "^3.4.0",
		"jest": "^24.8.0",
		"jest-circus": "^24.7.1",
