		"@types/node": "^18.11.11",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.44.0",
		"@vercel/ncc": "^0.34.0",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.3.1",
		"jest-circus": "^29.3.1",
